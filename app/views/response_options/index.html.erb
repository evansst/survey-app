<p id="notice"><%= notice %></p>

<h1>Response Options</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response_options.each do |response_option| %>
      <tr>
        <td><%= response_option.type %></td>
        <td><%= link_to 'Show', response_option %></td>
        <td><%= link_to 'Edit', edit_response_option_path(response_option) %></td>
        <td><%= link_to 'Destroy', response_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Response Option', new_response_option_path %>
